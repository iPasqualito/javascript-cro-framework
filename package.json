{
  "name": "javascript-cro-framework",
  "version": "1.0.0",
  "private": true,
  "description": "Vanilla JS CRO Framework",
  "main": "index.js",
  "scripts": {
    "dev": "npm run clean && parcel public/index.html --out-dir development -p 3000",
